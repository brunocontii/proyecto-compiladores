program {

    void print_int(integer x) extern;
    void print_bool(bool a) extern;

    integer pow(integer x, integer y) {
        integer z = x;
        integer w = y;

        if (x == 0) then {
            return 0;
        }

        if (y == 0) then {
            return 1;
        }

        if (y == 1) then {
            return x;
        }

        while (w > 1) {
            z = z * x;
            w = w - 1;
        }

        return z;
    }

    integer suma(integer x, integer y) {
        return x + y;
    }

    void main() {
        integer x = pow(suma(2,1),2+1);
        if (x == 27) then{
            print_bool(true);
        } else {
            print_bool(false);
        }
    }
}