program {

    bool esPar(integer x) {
        return x % 2 == 0;
    }

    integer pow(integer x, integer y) {
        integer z = 1;
        integer w = y;

        if (x < true || y > false) then {   // Primer error
            return -1;
        }

        if (x == 0) then {
            return 0;
        }

        if (true == 0) then {               // Segundo error
            return 1;
        }

        if (y == 1) then {
            return x;
        }

        while (w + 5) {                     // Tercer error
            z = z * x;
            w = w - 1;
        }

        return z;
    }

    integer factorial(integer x) {

        if (x - 0) then {                   // Cuarto error
            return -1;
        }

        if (x == 1 || x == 0) then {
            return 1;
        }

        return x + factorial(x-1);
    }

    void print_int(integer i) extern ;
    
    void main() {

        integer x = 2;
        integer y = 5 + - 2;
        integer u = false;                  // Quinto error
        bool par = esPar(pow(x, y));

        print_int(pow(x, y));

        if (!5) then {                       // Sexto error
            print_int(y);
        } else {
            integer z = pow(2, 5);
            return print_int(z);
        }

        if (x && y) then {                  // Septimo error
            print_int(0);
        } else {
            if (x || y) then {              // Octavo error
                print_int(x);
            } else {
                print_int(y);
            }
        }
    }
}