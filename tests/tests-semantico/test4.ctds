program {
    
    integer ifelse() extern;
    void whiletrue(integer x) extern;
    
    void main() {
        integer x = 5;
        integer y = 10;
        integer result = x + y;
        
        if (result > 12) then {
            integer temp = result * 2;
            whiletrue(temp);
        } else {
            whiletrue(result);
        }
        
        while (x < y) {
            x = x + 1;
        }
        
        return;
    }
}