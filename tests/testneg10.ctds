program {

    integer x = 2;
    integer y = 5 + - 2;

    bool esPar(integer x) {
        return x % 2 == 0;
    }

    integer pow(integer x, integer y) {
        integer  z   =   1;
        integer   w      =     y;

        if (!!(x == 0)) then {
            return 0;
        }

        if (y == 0) then {
            return 1;
        }

        if (y == 1) then {
            return x;
        }

        while (w > 0) {
            z = z * x;
            w = w - 1;
        }

        return z;
    }

    integer factorial(integer x) {

        if (x < 0) then {
            return -1;
        }

        if (x == 1 || x == 0) then {
            return 1;
        }

        return x + factorial(x-1);
    }

    void print_int(integer i) extern ;
    

    // El error estÃ¡ en que falta definir el main

    bool par = esPar(pow(x, y));

    print_int(pow(x, y));

    if (!par) then {
        print_int(y);
    } else {
        integer z = pow(2, 5);
        return print_int(z);
    }

    if (x == y) then {
        print_int(0);
    } else {
        if (x > y) then {
            print_int(x);
        } else {
            print_int(y);
        }
    }
}