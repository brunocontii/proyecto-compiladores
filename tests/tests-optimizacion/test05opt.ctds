program {

    integer f(integer a) {
        if (a > 0) then {
            if (a == 1) then {
                return 10;
                // inalcanzable
                a = a + 1;
            }
            return 5;
            // inalcanzable
            a = a - 1;
        } else {
            return 0;
            // inalcanzable
            a = 99;
        }
    }

    void print_int(integer x) extern;

    void main() {
        integer x = f(1);
        print_int(x);
        return;
        // inalcanzable
        print_int(999);
    }
}
