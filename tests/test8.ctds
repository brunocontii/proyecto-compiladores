program {

    integer suma(integer a, integer b) {
        return a + b;
    }

    bool esPar(integer x) {
        return x % 2 == 0;
    }

    void main() {
        integer x = suma(2,3);
        suma(2,3);

        if (4 < suma(2,3)) then {
            x = 0;
        }

        if (esPar(x)) then {
            x = 0;
        }
    }
}